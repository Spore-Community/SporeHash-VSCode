{
	"name": "sporehash",
	"displayName": "Spore Hash",
	"description": "Convert between names and FNV Hash IDs used in DBPF files for Spore, a 2008 video game from Maxis.",
	"version": "0.0.1",
	"publisher": "Spore-Community",
	"author": {
		"name": "Kyle Nanakdewa",
		"email": "spore@kylenanakdewa.com",
		"url": "https://kylenanakdewa.com"
	},
	"icon": "SporeHash.png",
	"galleryBanner": {
		"color": "#241F59",
		"theme": "dark"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/Spore-Community/SporeHash-VSCode"
	},
	"bugs": {
		"url": "https://github.com/Spore-Community/SporeHash-VSCode/issues",
		"email": "spore@kylenanakdewa.com"
	},
	"license": "SEE LICENSE IN LICENSE.txt",
	"engines": {
		"vscode": "^1.44.0"
	},
	"categories": [
		"Other"
	],
	"keywords": [
		"spore",
		"maxis",
		"dbpf",
		"fnv",
		"hash"
	],
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.44.0",
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"eslint": "^6.8.0",
		"@typescript-eslint/parser": "^2.26.0",
		"@typescript-eslint/eslint-plugin": "^2.26.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.1",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	}
}
